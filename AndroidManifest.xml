<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hostelease">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:label="HostelEase"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".StudentDashboardActivity"/>
        <activity android:name=".WardenDashboardActivity"/>
        <activity android:name=".LeaveRequestActivity"/>
        <activity android:name=".ComplaintActivity"/>
        <activity android:name=".AnnouncementActivity"/>
        <activity android:name=".MessMenuActivity"/>
    </application>
</manifest>
